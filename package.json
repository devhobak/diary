{
    "dependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.18",
        "concurrently": "^7.0.0",
        "express": "^4.17.3",
        "global": "^4.4.0",
        "react-router-dom": "^6.10.0",
        "recoil": "^0.7.7",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "scripts": {
        "server": "cd server && yarn start",
        "client": "cd client && yarn start",
